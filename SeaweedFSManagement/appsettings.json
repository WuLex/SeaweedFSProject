{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=RocketDb;Persist Security Info=True;User ID=sa;Password=********;TrustServerCertificate=true"
  },
  "SeaweedFS": {
    "ServerUrl": "http://localhost:9333",
    "UploadPath": "rocketimgs",
    "MasterHost": "your-master-host",
    "MasterPort": 9333, // 根据实际情况修改端口号
    //"Collection": "public_files",
    "ConnectionType": "Master" // 或 "Volume"，根据你的服务器配置选择
  }
}
